<!DOCTYPE html>
<html lang="en">


<head>
    <meta charset="UTF-8">
    <title>Electronics page</title>
    <link rel="shortcut icon" href="favicon.ico">
    <link rel="stylesheet" href="font-awesome.min.css">
    <link rel="stylesheet" href="style.css">
</head>

<head>

    <body>
        <header>
            <nav>

                <ul>
                    <li>
                        <a href="/women">Womens Collection</a>
                    </li>
                    <li>
                        <a href="/men">Mens Collection</a>
                    </li>
                    <li>
                        <a href="/jewelery">Jewelery Collection </a>
                    </li>
                    <li>
                        <a href="/electronics">Electronics Collection</a>
                    </li>
                </ul>
            </nav>
        </header>



        <ul class="electronicsSection">
            <% for(var i=0; i<cart.length; i++) {%>

                <% if(cart[i].category === "electronics") {%>

                    <li class="message <%= cart[i].priority %>">
                        <span id="storeItem">
                            <%= cart[i].title %>
                        </span>
                        <span>
                            <%= cart[i].description %>
                        </span>
                        <img src="<%= cart[i].image %>" alt="<%= cart[i].title %>">
                        <span>
                            <i class="fa fa-trash fa-2x" aria-hidden="true"></i>
                        </span>

                        <span>
                            <i class="fa fa fa-cart-plus fa-2x" aria-hidden="true"></i>
                        </span>



                    </li>
                    <% } %>
                        <% } %>
        </ul>



        <script src="main.js"></script>
    </body>

</html>